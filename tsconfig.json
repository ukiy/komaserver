{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": false,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "formatCodeOptions": {
        "indentSize": 4,
        "tabSize": 4,
        "newLineCharacter": "\n",
        "convertTabsToSpaces": true,
        "insertSpaceAfterCommaDelimiter": true,
        "insertSpaceAfterSemicolonInForStatements": true,
        "insertSpaceBeforeAndAfterBinaryOperators": true,
        "insertSpaceAfterKeywordsInControlFlowStatements": true,
        "insertSpaceAfterFunctionKeywordForAnonymousFunctions": true,
        "insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": false,
        "placeOpenBraceOnNewLineForFunctions": true,
        "placeOpenBraceOnNewLineForControlBlocks": true
    },
    "compileOnSave": true,
    "filesGlob": [
        "./**/*.ts",
        "./.tmp/**/*.d.ts",
        "!./node_modules/**/*.ts",
        "!./bower_components/**/*.ts",
        "!./typings/**/*.ts"
    ],
    "files": [
        "./src/app/components/githubContributor/githubContributor.service.ts",
        "./src/app/components/gogoshogi/game/game.directive.ts",
        "./src/app/components/gogoshogi/koma/koma.directive.ts",
        "./src/app/components/gogoshogi/koma/koma.ts",
        "./src/app/components/gogoshogi/mass/mass.directive.ts",
        "./src/app/components/gogoshogi/mass/mass.ts",
        "./src/app/components/gogoshogi/socket/socket.factory.ts",
        "./src/app/components/malarkey/malarkey.directive.ts",
        "./src/app/components/navbar/navbar.directive.ts",
        "./src/app/components/webDevTec/webDevTec.service.ts",
        "./src/app/index.config.ts",
        "./src/app/index.module.ts",
        "./src/app/index.route.ts",
        "./src/app/index.run.ts",
        "./src/app/main/main.controller.ts",
        "./.tmp/typings/angular-ui-router/angular-ui-router.d.ts",
        "./.tmp/typings/angularjs/angular-animate.d.ts",
        "./.tmp/typings/angularjs/angular-cookies.d.ts",
        "./.tmp/typings/angularjs/angular-mocks.d.ts",
        "./.tmp/typings/angularjs/angular-resource.d.ts",
        "./.tmp/typings/angularjs/angular-sanitize.d.ts",
        "./.tmp/typings/angularjs/angular.d.ts",
        "./.tmp/typings/jquery/jquery.d.ts",
        "./.tmp/typings/lodash/lodash.d.ts",
        "./.tmp/typings/moment/moment-node.d.ts",
        "./.tmp/typings/moment/moment.d.ts",
        "./.tmp/typings/socket.io-client/socket.io-client.d.ts",
        "./.tmp/typings/toastr/toastr.d.ts",
        "./.tmp/typings/tsd.d.ts"
    ]
}
